<?php
defined('BASEPATH') OR exit('No direct script access allowed');

class kategori_alat_dan_bahan extends CI_Controller {

	public function __construct()
	{
		parent::__construct();
		$this->fungsi->restrict();
		$this->load->model('master/m_kategori_alat_dan_bahan');
	}

	public function index()
	{
		$this->fungsi->check_previleges('kategori_alat_dan_bahan');
		$data['kategori_alat_dan_bahan'] = $this->m_kategori_alat_dan_bahan->getData();
		$this->load->view('master/kategori_alat_dan_bahan/v_kategori_alat_dan_bahan_list',$data);
    }
}
